{
  "parserOptions": { 
    "ecmaVersion":2020,
    "sourceType": "module"
  },
  "rules": { 
    "no-console": "warn", 
    "no-use-before-define": "warn", 
    "spaced-comment": "warn",
    "react/jsx-uses-react": "error",   
    "react/jsx-uses-vars": "error",
    "react/prop-types": "off"
  },
  "plugins": ["react"],
  "extends": ["standard","plugin:react/recommended"]
}
